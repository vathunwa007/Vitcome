@model List<Techer>
<style>
        body {
            background-image: url('https://images.pexels.com/photos/875858/pexels-photo-875858.png?auto=compress&cs=tinysrgb&dpr=2&h=650&w=940');
        }
    </style>
    <!---->
    <div class="container">
        <div class="row">
            <div class="mx-auto">
                <h1 style="color:white;">เลือกอาจารย์ที่ปรึกษาวิจัย</h1>
            </div>


            <table class="table table-hover">
                <thead>
                    <tr>
                        <th scope="col">#</th>
                        <th scope="col">ลำดับ</th>
                        <th scope="col">ชื่ออาจารย์</th>
                        <th scope="col">สกิล</th>
                        <th scope="col">สถานะ</th>

                    </tr>
                </thead>
                <tbody>
                    @for(int i=0; i
                    < Model.Count(); i++){ <tr onclick="window.open('Privacy','_self','');">
                        <th scope="row"><input type="checkbox" aria-label="Checkbox for following text input"> </th>
                        <td>@Model[i].Id</td>
                        <td>@Model[i].Name</td>
                        <td>@Model[i].Skill</td>
                        <td>@Model[i].Status</td>
                        </tr>
                        }


                </tbody>
            </table>



        </div>
    </div>