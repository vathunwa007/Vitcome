@model List<Techer>

    <div class="container">
        <div class="row">
            <div class="mx-auto">
                <h1>เลือกอาจารย์ที่ปรึกษาวิจัย</h1>
            </div>


            <table class="table table-hover">
                <thead>
                    <tr>
                        <th scope="col">#</th>
                        <th scope="col">ลำดับ</th>
                        <th scope="col">ชื่ออาจารย์</th>
                        <th scope="col">สกิล</th>
                        <th scope="col">สถานะ</th>

                    </tr>
                </thead>
                <tbody>
                    @for(int i=0; i < Model.Count(); i++){ 
                        <tr>
                        <th scope="row"><input type="checkbox" aria-label="Checkbox for following text input"> </th>
                        <td>@Model[i].Id</td>
                        <td>@Model[i].Name</td>
                        <td>@Model[i].Skill</td>
                        <td>@Model[i].Status</td>
                        </tr>
                        }


                </tbody>
            </table>

            <%@ Page Language="C#" Inherits="System.Web.UI.Page" %>

        </div>
    </div>